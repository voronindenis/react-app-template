{
  "name": "react-app-template",
  "displayName": "React Application Template",
  "version": "1.0.0",
  "description": "React Application Template",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_OPTIONS=--max-old-space-size=8192 NODE_ENV=development webpack-dev-server --mode development --host 0.0.0.0",
    "build": "cross-env NODE_ENV=production ./node_modules/.bin/webpack --mode production -p --config ./webpack.config.js",
    "test": "cross-env NODE_OPTIONS=--max-old-space-size=8192 TZ=UTC jest --coverage --passWithNoTests --verbose",
    "test-update": "cross-env NODE_OPTIONS=--max-old-space-size=8192 TZ=UTC jest -u --coverage",
    "test-watch": "cross-env NODE_OPTIONS=--max-old-space-size=8192 TZ=UTC jest -u --watch",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx ./src",
    "lint-fix": "eslint --cache --fix --ext .js,.jsx,.ts,.tsx ./src",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voronindenis/react-app-template.git"
  },
  "keywords": [
    "react"
  ],
  "author": "Denis Voronin <mystimind@mail.ru>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/voronindenis/react-app-template/issues"
  },
  "homepage": "https://github.com/voronindenis/react-app-template#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test"
    }
  },
  "engines": {
    "node": ">= 10"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@storybook/addon-actions": "^6.0.26",
    "@storybook/addon-essentials": "^6.0.26",
    "@storybook/addon-links": "^6.0.26",
    "@storybook/react": "^6.0.26",
    "@types/enzyme": "^3.10.7",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.15",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.6",
    "@types/redux-mock-store": "^1.0.2",
    "@types/redux-persist": "^4.3.1",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "autoprefixer": "^10.0.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^26.6.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-rename-import": "^2.3.0",
    "copy-webpack-plugin": "^6.2.1",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.0.0",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "jest": "^26.6.0",
    "jest-css-modules-transform": "^4.0.1",
    "jest-runner-concurrent": "^0.1.1",
    "loader-utils": "^2.0.0",
    "postcss-loader": "^4.0.4",
    "precss": "^4.0.0",
    "sass-loader": "^10.0.3",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.1.3",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.11.0",
    "webpack-pwa-manifest": "^4.2.0"
  },
  "dependencies": {
    "@devexperts/remote-data-ts": "^2.0.4",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "core-js": "^3.6.5",
    "fp-ts": "^2.8.4",
    "io-ts": "^2.2.11",
    "localforage": "^1.9.0",
    "raf-polyfill": "^1.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "regenerator-runtime": "^0.13.7"
  }
}
